#include "vector2.hpp"

Vector2::Vector2(double _x, double _y) {
    x = _x;
    y = _y;
}

double Vector2::getX() const {
    return x;
}

double Vector2::getY() const {
    return y;
}

void Vector2::operator+=(const Vector2& v) {
    x += v.x;
    y += v.y;
}

void Vector2::operator-=(const Vector2& v) {
    x -= v.x;
    y -= v.y;
}